<h1>Editar Aljibe</h1>

<div class="row">
  <div class="">
    <%= form_for(@tank) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_area :description %>

      <%= f.label :latitude %>
      <%= f.text_field :latitude %>

      <%= f.label :longitude %>
      <%= f.text_field :longitude %>

      <%= f.label :notes %>
      <%= f.text_area :notes %>      

      <%= f.submit "Guardar cambios", class: "" %>
    <% end %>
  </div>
</div>